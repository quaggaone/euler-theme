                    {% if elrTemplate == "partner" %}
                        <div class="{{ euler.abbr }}-meta__table">
                            <table class="{{ euler.abbr }}-table">
                                <tbody>
                                    <tr>
                                        <th scope="row">Titel</th>
                                        <td>Value</td>
                                    </tr>
                                    <tr>
                                        <th scope="row">Titel</th>
                                        <td>Value</td>
                                    </tr>
                                    <tr>
