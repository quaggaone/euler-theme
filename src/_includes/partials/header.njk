            </div>
            <div class="{{ euler.abbr }}-meta__container">
                <div class="{{ euler.abbr }}-meta__col {{ euler.abbr }}-meta__col--side-start">
                    {% if elrTemplate == "book" %}
                        <div class="{{ euler.abbr }}-meta__media">
                            <img src="{{ euler.assetsDir }}brand/open-graph-vertical.png" alt="">
                        </div>
                    {% endif %}
                    {% if elrTemplate == "partner" %}
                        <div class="{{ euler.abbr }}-meta__media">
                            <img src="{{ euler.assetsDir }}brand/euler-logo-inline-black.png" alt="">
